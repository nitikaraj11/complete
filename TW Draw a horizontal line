import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
    
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter number of asterisks ");
        int  n = sc.nextInt();
        for(int i=0;i<n;i++){
          
	  System.out.print("*");
	}
	sc.close();
    }
}
